#include <iostream>
using namespace std;

int main() {
	// your code goes here
	string s;
	cin>>s;
	int n;
	cin>>n;
	
	while(n--){
	    string s1;
	    cin>>s1;
	    int count=0;
	    for(int i=0;i<s.length();i++){
	        for(int j=0;j<s1.length();j++){
	        if(s[i]==s1[j]){
	            count++;
	        }
	    }
	}
	count==s1.length()?cout<<"Yes\n":cout<<"No\n";
	}
	return 0;
}
